<addon id="plugin.video.selfless" name="[B]Self Less Live[/B]" version="2.2" provider-name="Altruistic / Kodi Ghost">
	<requires>
		<import addon="xbmc.python" version="2.1.0" />
		<import addon="script.module.pyxbmct" />
		<import addon="script.selflesslive.artwork" />
		<import addon="script.selflesslive.metadata" />
		<import addon="script.module.urlresolver" version="4.0.05" />
		<import addon="script.module.liveresolver" version="0.1.49" />
		<import addon="script.module.livenettv.net" />
		<import addon="script.module.mobdro" />
		<import addon="script.module.swiftstreamz" />
		<import addon="script.module.tt" />
		<import addon="plugin.video.tvone1" />
		<import addon="plugin.video.tvone11" />
		<import addon="plugin.video.tvone111" />
		
	</requires>
	<extension point="xbmc.python.pluginsource" library="selflesslive.py">
		<provides>video</provides>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary lang="en">Hey One and All, Search engine for Internet Tv Channels available in the open Internet places.</summary>
    <description lang="en">Hey One and All, Search engine for Internet Tv Channels available in the open Internet places.</description>
	<disclaimer lang="en">Hey One and All, Search engine for Internet Tv Channels available in the open Internet places.</disclaimer>
		<platform>all</platform>
		<forum>-</forum>
		<website>-</website>
		<assets>
			<icon>icon.png</icon>
			<fanart>fanart.jpg</fanart>
			<screenshot>resources/screenshots/screen1.jpg</screenshot>
		</assets>
		<news></news>
	</extension>
</addon>

